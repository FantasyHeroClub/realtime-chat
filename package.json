{
  "name": "react-charts",
  "description": "Charts for React",
  "author": "Dan Delany <dand@spotify.com>",
  "version": "0.0.3",
  "main": "lib/index.js",
  "scripts": {
    "dev": "webpack-dev-server --config webpack.config.dev.js",
    "build-lib": "rm -rf lib/ && babel src --out-dir lib",
    "build-examples": "webpack --config webpack.config.base.js",
    "build-tests": "webpack --config webpack.config.test.js",
    "build": "npm run build-lib && npm run build-examples && npm run build-tests",
    "serve": "python -m SimpleHTTPServer",
    "test": "npm run build-tests && node_modules/.bin/mocha-phantomjs -p $(which phantomjs) tests/index.html"
  },
  "dependencies": {
    "d3": "^3.5.6",
    "jquery": "^2.1.4",
    "lodash": "^3.10.1",
    "moment": "^2.10.6",
    "numeral": "^1.5.3"
  },
  "peerDependencies": {
    "react": "^0.13.3"
  },
  "devDependencies": {
    "babel-cli": "^6.1.1",
    "babel-core": "^6.0.20",
    "babel-loader": "^6.0.1",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "chai": "^3.4.0",
    "css-loader": "^0.15.6",
    "json-loader": "^0.5.2",
    "less": "^2.5.1",
    "less-loader": "^2.2.0",
    "mocha": "^2.3.3",
    "mocha-phantomjs": "^4.0.1",
    "react-hot-loader": "^1.2.8",
    "react": "^0.13.3",
    "style-loader": "^0.12.3",
    "webpack": "^1.10.5",
    "webpack-dev-server": "^1.10.1"
  },
  "license": "proprietary",
  "repository": {
    "type": "git",
    "url": "https://ghe.spotify.net/dand/react-charts"
  },
  "publishConfig": {
    "registry": "http://npm-registry.spotify.net"
  }
}
